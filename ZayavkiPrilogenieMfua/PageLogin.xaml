<Page x:Class="ZayavkiPrilogenieMfua.PageLogin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ZayavkiPrilogenieMfua"
      mc:Ignorable="d" 
      d:DesignHeight="343" d:DesignWidth="421"
      Title="PageLogin">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Label Content="Логин" Height="45" FontSize="24" Foreground="Black"  HorizontalAlignment="Center"/>
        <TextBox x:Name="NameBox"  Width="150"/>
        <Label Content="Пароль" Height="45" FontSize="24" Foreground="Black"  HorizontalAlignment="Center"/>
        <TextBox x:Name="PasseBox"  Width="150"/>
        <Label Height="16" FontSize="24" Foreground="Black"  HorizontalAlignment="Center"/>
        <Button x:Name="button" Height="25"  Content="Войти" FontSize="16" Width="212" Click="Loginbutton_Click"/>
    </StackPanel>
</Page>
